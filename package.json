{
  "name": "type-insurance",
  "version": "1.0.0",
  "description": "Helper class to force (input) types, mainly for pure JavaScript environments",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "npm run prepublish && jest",
    "test-dev": "xo && npm run test",
    "prepublish": "babel src --out-dir ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giannotr/type-insurance.git"
  },
  "keywords": [
    "data",
    "interface",
    "type",
    "types",
    "convert",
    "safety"
  ],
  "author": "Ruben Giannotti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/giannotr/type-insurance/issues"
  },
  "homepage": "https://github.com/giannotr/type-insurance#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "jest": "^25.3.0",
    "xo": "^0.29.1"
  },
  "dependencies": {
    "baseconvert": "^1.0.0",
    "hash.js": "^1.1.7",
    "lodash": "^4.17.15"
  },
  "xo": {
    "rules": {
      "keyword-spacing": "warn",
      "no-trailing-spaces": "warn",
      "no-prototype-builtins": "warn",
      "no-lonely-if": "off",
      "no-else-return": "off",
      "no-implicit-coercion": "off"
    },
    "ignores": [
      "./*.js",
      "./src/*.test.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/src/"
    ]
  }
}
